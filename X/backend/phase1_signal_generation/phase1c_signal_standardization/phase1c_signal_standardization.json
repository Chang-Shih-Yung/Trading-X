{
  "strategy_dependency_graph": {
    "strategy_name": "Phase1C 信號標準化引擎（實戰級）",
    "description": "統一信號格式化、品質評分、優先級排序與輸出標準化系統 - 延遲感知、跨市場驗證、ML適應",
    "version": "2.1.0",
    "dependencies": {
      "core_dependencies": [
        "phase1b_volatility_adaptation",
        "indicator_dependency_graph",
        "pandas_ta_indicators"
      ],
      "external_libraries": ["numpy", "pandas", "logging", "datetime", "scipy"],
      "market_regime_detector": "phase3_market_analyzer",
      "cross_market_data": [
        "btc_correlation",
        "total_market_cap",
        "liquidity_metrics"
      ],
      "signal_sources": [
        "phase1b_volatility_regime_signals",
        "technical_indicator_signals",
        "volume_confirmation_signals"
      ],
      "cross_module_dependencies": [
        "phase3_market_analyzer",
        "unified_signal_candidate_pool"
      ]
    },
    "inherits_configuration_from": "indicator_dependency_graph",
    "uses_timeframes": "configurable_parameters.timeframes",
    "input_format_compatibility": {
      "expects_from_phase1b": "{symbol}_{timeframe}_volatility_regime",
      "expects_from_indicator_graph": "{symbol}_{timeframe}_{indicator_name}",
      "processes_multiple_signal_types": true,
      "requires_quality_scoring": true,
      "cross_market_validation": true
    },
    "computation_flow": {
      "layer_0_cross_module_sync": {
        "description": "跨模組時間戳統一同步層 - 解決上游數據同步衝突",
        "dependencies": [],
        "sync_tolerance_ms": 200,
        "implementation": {
          "shared_timestamp_source": "system_utc_with_exchange_offset",
          "sync_validation": "all_modules_use_same_reference",
          "fallback_strategy": "use_latest_valid_timestamp"
        },
        "integration_points": {
          "phase1b_sync": "inherit_from_cross_module_sync",
          "indicator_graph_sync": "inherit_from_cross_module_sync",
          "validation": "verify_all_inputs_synchronized"
        },
        "output": "synchronized_timestamp_reference",
        "computation_time_ms": 1
      },
      "layer_1_signal_collection": {
        "description": "信號收集與初步驗證層 - 延遲感知採樣",
        "dependencies": ["layer_0_cross_module_sync"],
        "market_regime_aware_processing": {
          "extreme_market_detection": {
            "btc_rapid_move_threshold": "> 2% in 5min",
            "trigger_high_frequency_sampling": "5s mini-batch processing",
            "priority_signal_fast_track": "< 2ms for tier_1_candidates"
          },
          "anomaly_isolation": {
            "z_score_filtering": {
              "enabled": true,
              "threshold": 3.0,
              "isolation_tag": "ANOMALY_CANDIDATE"
            },
            "mad_filtering": {
              "enabled": true,
              "threshold": 2.5,
              "description": "Median Absolute Deviation 離群檢測"
            }
          }
        },
        "operations": {
          "signal_format_adapter": {
            "description": "上游信號格式標準化適配器 - 解決格式不統一問題",
            "input": "raw_upstream_signals",
            "adapters": {
              "phase1b_adapter": {
                "input_format": "{symbol}_{timeframe}_volatility_regime",
                "output_mapping": {
                  "strategy": "volatility_adaptation",
                  "signal_type": "extract_from_regime_type",
                  "mapping_rules": {
                    "VOLATILITY_BREAKOUT": "BREAKOUT",
                    "VOLATILITY_MEAN_REVERSION": "MEAN_REVERSION",
                    "VOLATILITY_REGIME_CHANGE": "REGIME_CHANGE"
                  }
                }
              },
              "indicator_adapter": {
                "input_format": "{symbol}_{timeframe}_{indicator_name}",
                "output_mapping": {
                  "strategy": "technical_analysis",
                  "signal_type": "derive_from_indicator_signal",
                  "mapping_rules": {
                    "RSI_oversold|RSI_overbought": "MEAN_REVERSION",
                    "MACD_bullish|MACD_bearish": "TREND_FOLLOWING",
                    "BB_breakout": "BREAKOUT",
                    "volume_confirmation": "TECHNICAL_CONVERGENCE"
                  }
                }
              }
            },
            "output": "format_standardized_signals",
            "computation_time_ms": 2
          },
          "quality_score_standardization": {
            "description": "統一品質評分轉換機制 - 解決評分標準差異",
            "input": "format_standardized_signals",
            "conversions": {
              "phase1b_to_standard": {
                "false_breakout_probability": "1.0 - false_breakout_probability",
                "signal_stability_score": "signal_stability_score",
                "multi_confirmation_status": {
                  "CONFIRMED": 1.0,
                  "PARTIAL": 0.7,
                  "NONE": 0.4
                },
                "composite_formula": "min(1.0, (1-false_breakout) * stability * confirmation_factor)"
              },
              "indicator_to_standard": {
                "numerical_quality": "use_directly_if_0_to_1_range",
                "fallback_quality": "0.6_default_for_missing_scores"
              }
            },
            "output": "quality_standardized_signals",
            "computation_time_ms": 1
          },
          "volatility_signal_collection": {
            "input": "quality_standardized_signals",
            "signal_types": [
              "VOLATILITY_BREAKOUT",
              "VOLATILITY_MEAN_REVERSION",
              "VOLATILITY_REGIME_CHANGE"
            ],
            "validation": {
              "signal_strength_range": "0.0-1.0",
              "execution_priority_check": "HIGH|MEDIUM|LOW",
              "timestamp_validation": "ISO_datetime_format",
              "source_verification": "phase1b_volatility_adaptation_v2"
            },
            "output": "validated_volatility_signals",
            "computation_time_ms": 2
          },
          "technical_indicator_signal_collection": {
            "input": "quality_standardized_signals",
            "indicators": [
              "RSI_signals",
              "MACD_signals",
              "BB_position_signals",
              "volume_confirmation_signals"
            ],
            "quality_filtering": {
              "minimum_quality_score": 0.6,
              "multi_indicator_confirmation": true,
              "data_sync_verification": "layer_0_cross_module_sync"
            },
            "output": "validated_technical_signals",
            "computation_time_ms": 3
          },
          "enhanced_conflict_prevention": {
            "description": "預防上游信號衝突的智能機制",
            "input": ["validated_volatility_signals", "validated_technical_signals"],
            "conflict_detection": {
              "temporal_conflict": {
                "detection_window": "30s",
                "phase1b_vs_indicator_signals": "check_contradictory_directions",
                "resolution_strategy": "weighted_confidence_based_selection"
              },
              "quality_conflict": {
                "low_quality_phase1b_vs_high_quality_indicator": "prefer_indicator",
                "high_quality_phase1b_vs_low_quality_indicator": "prefer_phase1b",
                "similar_quality": "use_signal_diversity_preference"
              }
            },
            "preprocessing_optimization": {
              "upstream_signal_pre_filtering": {
                "phase1b_minimum_stability": 0.7,
                "indicator_minimum_quality": 0.6,
                "early_rejection": "reduce_downstream_processing_load"
              }
            },
            "output": "conflict_resolved_signals",
            "computation_time_ms": 2
          },
          "signal_integrity_check": {
            "input": "conflict_resolved_signals",
            "checks": {
              "timestamp_alignment": "所有信號時間戳對齊檢查",
              "duplicate_detection": "重複信號識別與去除",
              "signal_conflict_detection": "矛盾信號識別",
              "data_completeness": "信號數據完整性檢查"
            },
            "conflict_resolution": {
              "priority_based": "higher_priority_signal_wins",
              "quality_based": "higher_quality_score_wins",
              "timestamp_based": "latest_signal_wins"
            },
            "intelligent_conflict_handling": {
              "conflicting_signal_diversion": {
                "enabled": true,
                "examples": "RSI_oversold + Phase1B_breakout 同時觸發",
                "action": "route_to_secondary_validation_module",
                "validation_timeout": "3s max"
              },
              "cross_exchange_alignment": {
                "enabled": true,
                "latency_compensation": "binance_futures vs binance_spot time_diff",
                "max_acceptable_drift": "200ms"
              }
            },
            "output": "integrity_verified_signals",
            "computation_time_ms": 2
          }
        }
      },
      "layer_2_signal_standardization": {
        "description": "信號標準化與格式統一層 - 跨市場驗證",
        "dependencies": ["layer_0_cross_module_sync", "layer_1_signal_collection"],
        "operations": {
          "signal_format_standardization": {
            "input": "integrity_verified_signals",
            "standardization": {
              "naming_convention": "{symbol}_{timeframe}_{strategy}_{signal_type}",
              "signal_strength_normalization": "0.0-1.0 統一範圍",
              "confidence_score_calculation": "基於多因子品質評分",
              "execution_priority_mapping": "HIGH/MEDIUM/LOW 標準化"
            },
            "unified_format": {
              "signal_id": "unique_identifier",
              "symbol": "trading_pair",
              "timeframe": "analysis_timeframe",
              "strategy": "signal_generation_strategy",
              "signal_type": "signal_category",
              "signal_strength": "0.0-1.0",
              "confidence_score": "0.0-1.0",
              "execution_priority": "HIGH|MEDIUM|LOW",
              "timestamp": "ISO_datetime",
              "source": "strategy_module_version"
            },
            "output": "standardized_signals",
            "computation_time_ms": 4
          },
          "quality_score_enhancement": {
            "input": "standardized_signals",
            "enhancement_factors": {
              "multi_timeframe_confirmation": {
                "enabled": true,
                "weight": 0.25,
                "description": "多時間框架確認加成"
              },
              "volume_confirmation": {
                "enabled": true,
                "weight": 0.2,
                "description": "成交量確認加成"
              },
              "technical_convergence": {
                "enabled": true,
                "weight": 0.25,
                "description": "技術指標收斂確認"
              },
              "market_sentiment_alignment": {
                "enabled": true,
                "weight": 0.15,
                "description": "市場情緒一致性"
              },
              "historical_accuracy": {
                "enabled": true,
                "weight": 0.15,
                "description": "歷史準確率加權",
                "dynamic_weighting": {
                  "low_volatility_period": "reduce_volatility_strategy_weight by 30%",
                  "high_volatility_period": "increase_volatility_strategy_weight by 20%",
                  "market_regime_adaptive": true
                }
              }
            },
            "cross_market_validation": {
              "enabled": true,
              "btc_correlation_check": {
                "for_altcoins": "check_btc_totalcap_synchronization",
                "correlation_threshold": 0.7,
                "boost_factor": 1.15
              },
              "liquidity_weighting": {
                "formula": "24h_volume / order_book_depth",
                "low_liquidity_penalty": "multiply by 0.85",
                "high_liquidity_boost": "multiply by 1.1"
              }
            },
            "trading_session_adaptation": {
              "asian_session": {
                "technical_convergence_weight": 0.35,
                "volume_weight": 0.15
              },
              "european_session": {
                "technical_convergence_weight": 0.25,
                "volume_weight": 0.25
              },
              "american_session": {
                "volume_weight": 0.35,
                "technical_convergence_weight": 0.15
              }
            },
            "quality_calculation": {
              "base_score": "original_signal_quality",
              "enhancement_multiplier": "weighted_sum_of_factors",
              "session_adjustment": "trading_session_specific_weights",
              "liquidity_adjustment": "liquidity_weighting_factor",
              "final_score": "min(1.0, base_score * enhancement_multiplier * session_adjustment * liquidity_adjustment)",
              "confidence_boost": "quality_score_based_confidence_adjustment"
            },
            "output": "enhanced_quality_signals",
            "computation_time_ms": 6
          },
          "signal_metadata_enrichment": {
            "input": "enhanced_quality_signals",
            "metadata": {
              "market_context": {
                "current_volatility": "absolute_volatility_level",
                "volatility_percentile": "timeframe_specific_percentile",
                "market_activity_level": "HIGH|NORMAL|LOW",
                "trading_session": "ASIAN|EUROPEAN|AMERICAN|OVERLAP",
                "btc_correlation": "current_correlation_coefficient",
                "market_regime": "trending|sideways|volatile"
              },
              "risk_metrics": {
                "signal_risk_level": "LOW|MEDIUM|HIGH",
                "recommended_position_size": "volatility_adjusted_size",
                "stop_loss_suggestion": "atr_based_stop_loss",
                "take_profit_suggestion": "risk_reward_optimized",
                "liquidity_score": "0.0-1.0"
              },
              "execution_guidance": {
                "optimal_execution_timeframe": "strategy_recommended_timeframe",
                "execution_conditions": "market_state_requirements",
                "contraindications": "avoid_execution_conditions"
              }
            },
            "output": "enriched_metadata_signals",
            "computation_time_ms": 3
          }
        }
      },
      "layer_3_signal_prioritization": {
        "description": "信號優先級排序與篩選層 - 動態調整",
        "dependencies": ["layer_0_cross_module_sync", "layer_2_signal_standardization"],
        "extreme_market_fast_track": {
          "description": "極端市場快速通道 - 解決延遲預算超標問題",
          "detection_threshold": "btc_move > 2% in 5min",
          "tier_1_fast_track": {
            "target_latency_ms": 15,
            "allocation": {
              "signal_collection": "3ms (cached_volatility + priority_indicators)",
              "format_adaptation": "2ms (pre_computed_mappings)",
              "quality_standardization": "1ms (lookup_tables)",
              "basic_scoring": "4ms (essential_dimensions_only)",
              "immediate_output": "3ms (tier_1_bypass_complex_scoring)",
              "buffer": "2ms"
            },
            "bypass_complex_processing": true,
            "essential_scoring_only": true
          }
        },
        "operations": {
          "multi_dimensional_scoring": {
            "input": "enriched_metadata_signals",
            "scoring_dimensions": {
              "signal_strength": {
                "weight": 0.3,
                "range": "0.0-1.0",
                "description": "信號本身強度"
              },
              "confidence_score": {
                "weight": 0.25,
                "range": "0.0-1.0",
                "description": "信號可信度評分"
              },
              "execution_priority": {
                "weight": 0.2,
                "mapping": { "HIGH": 1.0, "MEDIUM": 0.6, "LOW": 0.3 },
                "description": "執行優先級權重"
              },
              "market_timing": {
                "weight": 0.15,
                "factors": [
                  "volatility_alignment",
                  "session_activity",
                  "volume_confirmation"
                ],
                "description": "市場時機適配度"
              },
              "risk_reward_ratio": {
                "weight": 0.1,
                "calculation": "expected_profit / expected_risk",
                "description": "風險報酬比評估",
                "high_ratio_boost": "if risk_reward > 3.0: apply 1.2x multiplier"
              }
            },
            "dynamic_priority_adjustment": {
              "rapid_market_movement": {
                "volatility_threshold": "> 95th percentile",
                "tier_2_promotion": "promote_to_execution_layer",
                "description": "快速變動時 Tier 2 臨時提升"
              },
              "signal_scarcity_boost": {
                "low_signal_frequency": "< 3 signals per 15min",
                "individual_signal_weight_boost": 1.25,
                "description": "低信號頻率時提高單一信號權重"
              }
            },
            "composite_score": "weighted_sum_of_dimensions",
            "output": "multi_dimensional_scored_signals",
            "computation_time_ms": 5
          },
          "signal_ranking_algorithm": {
            "input": "multi_dimensional_scored_signals",
            "ranking_method": "composite_score_descending",
            "tie_breaking": {
              "primary": "higher_confidence_score",
              "secondary": "higher_signal_strength",
              "tertiary": "latest_timestamp"
            },
            "ranking_categories": {
              "tier_1_signals": "composite_score >= 0.8",
              "tier_2_signals": "0.6 <= composite_score < 0.8",
              "tier_3_signals": "0.4 <= composite_score < 0.6",
              "filtered_signals": "composite_score < 0.4"
            },
            "output": "ranked_signal_tiers",
            "computation_time_ms": 2
          },
          "signal_filtering_rules": {
            "input": "ranked_signal_tiers",
            "filtering_criteria": {
              "minimum_quality_threshold": 0.6,
              "maximum_signals_per_symbol": 3,
              "maximum_signals_per_timeframe": 5,
              "signal_diversity_requirement": "different_signal_types_preferred",
              "temporal_filtering": "avoid_signals_within_5min"
            },
            "reverse_signal_conflict_suppression": {
              "enabled": true,
              "conflict_detection_window": "1min",
              "action": "reduce_both_scores by 20% or delay_transmission",
              "example": "BUY signal followed by SELL signal within 1min"
            },
            "machine_learning_adaptive_thresholds": {
              "enabled": true,
              "threshold_adjustment": {
                "trending_market": "lower minimum_quality_threshold to 0.55",
                "sideways_market": "raise minimum_quality_threshold to 0.65",
                "volatile_market": "dynamic threshold based on volatility percentile"
              },
              "historical_performance_learning": "adjust weights based on 7-day rolling accuracy"
            },
            "filtering_logic": {
              "quality_filter": "remove_signals_below_threshold",
              "quantity_filter": "keep_top_n_per_category",
              "diversity_filter": "prefer_complementary_signals",
              "temporal_filter": "remove_too_recent_duplicates",
              "conflict_filter": "apply_reverse_signal_suppression"
            },
            "output": "filtered_high_quality_signals",
            "computation_time_ms": 2
          }
        }
      },
      "layer_4_output_generation": {
        "description": "最終輸出生成與分發層 - 流式輸出",
        "dependencies": ["layer_0_cross_module_sync", "layer_3_signal_prioritization"],
        "operations": {
          "standardized_output_formatting": {
            "input": "filtered_high_quality_signals",
            "output_formats": {
              "unified_signal_format": {
                "structure": "standardized_trading_signal",
                "destination": "unified_signal_candidate_pool",
                "format": "json_structured"
              },
              "execution_ready_format": {
                "structure": "execution_optimized_signal",
                "destination": "trading_execution_engine",
                "format": "minimal_latency_format"
              },
              "analysis_format": {
                "structure": "detailed_analysis_signal",
                "destination": "signal_analysis_dashboard",
                "format": "comprehensive_metadata"
              }
            },
            "output": "multi_format_signals",
            "computation_time_ms": 3
          },
          "signal_distribution": {
            "input": "multi_format_signals",
            "streaming_output": {
              "enabled": true,
              "tier_1_immediate_push": "< 2ms latency",
              "tier_2_batch_push": "every 5s",
              "tier_3_scheduled_push": "every 15s",
              "description": "流式推送避免等待所有計算完成"
            },
            "distribution_channels": {
              "primary_channel": {
                "destination": "unified_signal_candidate_pool",
                "priority": "immediate",
                "format": "unified_signal_format"
              },
              "execution_channel": {
                "destination": "trading_execution_engine",
                "priority": "high",
                "format": "execution_ready_format",
                "filtering": "tier_1_signals_only"
              },
              "monitoring_channel": {
                "destination": "signal_performance_monitor",
                "priority": "normal",
                "format": "analysis_format"
              },
              "backup_channel": {
                "destination": "signal_archive_storage",
                "priority": "low",
                "format": "comprehensive_backup"
              }
            },
            "distribution_logic": {
              "tier_1_signals": "all_channels",
              "tier_2_signals": "primary_and_monitoring_channels",
              "tier_3_signals": "monitoring_and_backup_channels"
            },
            "output": "distributed_signals",
            "computation_time_ms": 2
          },
          "performance_metrics_logging": {
            "input": "distributed_signals",
            "real_time_backtesting": {
              "enabled": true,
              "backtest_intervals": ["1min", "5min", "15min"],
              "accuracy_tracking": "compare_signal_vs_actual_price_movement",
              "feedback_loop": "adjust_quality_calculation_weights",
              "description": "即時回測生成真實命中率數據"
            },
            "execution_feedback_integration": {
              "enabled": true,
              "feedback_metrics": [
                "order_fill_success_rate",
                "slippage_percentage",
                "post_execution_pnl",
                "time_to_market_move"
              ],
              "weight_adjustment": "auto_adjust_signal_strength_calculation",
              "description": "交易執行引擎回饋迴路"
            },
            "latency_distribution_monitoring": {
              "track_percentiles": ["P50", "P95", "P99"],
              "sla_target": "< 50ms for P99",
              "alert_threshold": "P95 > 40ms",
              "description": "延遲分佈監測確保極端情況可控"
            },
            "metrics": {
              "signal_generation_metrics": {
                "total_signals_processed": "count",
                "signals_by_tier": "tier_distribution",
                "average_quality_score": "mean_quality",
                "processing_time": "total_computation_time",
                "real_time_accuracy": "1min/5min/15min hit_rate"
              },
              "quality_metrics": {
                "multi_confirmation_rate": "percentage_with_multiple_confirmations",
                "average_confidence_score": "mean_confidence",
                "signal_diversity_index": "shannon_entropy_of_signal_types",
                "cross_market_correlation": "btc_altcoin_signal_alignment"
              },
              "performance_metrics": {
                "computation_time_by_layer": "layer_timing_breakdown",
                "memory_usage": "peak_memory_consumption",
                "cache_hit_rate": "signal_processing_cache_efficiency",
                "streaming_latency": "tier_1_push_latency_p99"
              }
            },
            "logging_destinations": {
              "performance_database": "structured_metrics",
              "monitoring_dashboard": "real_time_kpis",
              "log_files": "detailed_trace_logs",
              "ml_training_pipeline": "accuracy_and_feedback_data"
            },
            "output": "performance_logs",
            "computation_time_ms": 2
          }
        }
      }
    },
    "signal_output_format": {
      "format_compatibility": {
        "naming_convention": "{symbol}_{timeframe}_{strategy}_{signal_type}",
        "inherits_from": "indicator_dependency_graph_v2",
        "output_standardization": "unified_signal_format_v2.1"
      },
      "standardized_trading_signal": {
        "signal_id": "unique_identifier_uuid",
        "symbol": "trading_pair (e.g., BTCUSDT)",
        "timeframe": "analysis_timeframe (e.g., 1m, 5m, 15m, 1h)",
        "strategy": "signal_generation_strategy (e.g., volatility_adaptation, technical_convergence)",
        "signal_type": "BREAKOUT | MEAN_REVERSION | REGIME_CHANGE | TECHNICAL_CONVERGENCE",
        "signal_strength": "0.0-1.0 (標準化信號強度)",
